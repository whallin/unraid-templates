<?xml version="1.0"?>
<Container version="2">
    <!-- App info -->
    <Name>wh-haptic</Name>
    <Overview>A new local-first, privacy-focused and open-source home for your markdown notes.</Overview>
    <Category>Productivity: Other:</Category>
    <Beta>false</Beta>
    <!-- Image settings -->
    <Repository>chroxify/haptic-web:latest</Repository>
    <Registry>https://hub.docker.com/r/chroxify/haptic-web</Registry>
    <Project>https://haptic.md/</Project>
    <!-- Docker settings -->
    <WebUI>http://[IP]:[PORT:80]/</WebUI>
    <ExtraParams>--init --no-healthcheck --restart=always</ExtraParams>
    <Network>bridge</Network>
    <Privileged>false</Privileged>
    <PrivilegedReq>false</PrivilegedReq>
    <Shell>bash</Shell>
    <!-- Template settings -->
    <GitHub>https://github.com/whallin/unraid-templates</GitHub>
    <ReadMe>https://github.com/whallin/unraid-templates</ReadMe>
    <Support>https://github.com/whallin/unraid-templates/issues</Support>
    <Icon>https://raw.githubusercontent.com/whallin/unraid-templates/main/whallin/imgs/haptic.png</Icon>
    <TemplateURL>https://raw.githubusercontent.com/whallin/unraid-templates/main/whallin/haptic.xml</TemplateURL>
    <Date>2000-01-01</Date>
    <!-- Ports -->
    <Config Name="Haptic web UI port" Target="80" Default="80" Mode="tcp" Description="The TCP port Haptic listens on." Type="Port" Display="always" Required="true" Mask="false" />
</Container>