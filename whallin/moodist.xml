<?xml version="1.0"?>
<Container version="2">
    <!-- App info -->
    <Name>wh-moodist</Name>
    <Overview>Ambient sounds for focus and calm.</Overview>
    <Category>Tools: Other: MediaApp:Music</Category>
    <Beta>false</Beta>
    <!-- Image settings -->
    <Repository>ghcr.io/remvze/moodist:latest</Repository>
    <Registry>https://github.com/remvze/moodist/pkgs/container/moodist</Registry>
    <Project>https://moodist.app/</Project>
    <!-- Docker settings -->
    <WebUI>http://[IP]:[PORT:8080]/</WebUI>
    <ExtraParams>--init --no-healthcheck</ExtraParams>
    <Network>bridge</Network>
    <Privileged>false</Privileged>
    <PrivilegedReq>false</PrivilegedReq>
    <Shell>bash</Shell>
    <!-- Template settings -->
    <GitHub>https://github.com/whallin/unraid-templates</GitHub>
    <ReadMe>https://github.com/whallin/unraid-templates</ReadMe>
    <Support>https://github.com/whallin/unraid-templates/issues</Support>
    <Icon>https://raw.githubusercontent.com/whallin/unraid-templates/main/whallin/imgs/moodist.png</Icon>
    <TemplateURL>https://raw.githubusercontent.com/whallin/unraid-templates/main/whallin/moodist.xml</TemplateURL>
    <Date>2000-01-01</Date>
    <!-- Ports -->
    <Config Name="Moodist web UI port" Target="8080" Default="8080" Mode="tcp" Description="The TCP port Moodist listens on." Type="Port" Display="always" Required="true" Mask="false">8080</Config>
</Container>