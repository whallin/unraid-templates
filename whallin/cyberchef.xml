<?xml version="1.0"?>
<Container version="2">
    <!-- Replace 'APPNAME' with the name of the application -->
    <!-- Replace 'CACATEGORY' with the CA category for the application -->
    <!-- Replace 'REPONAME' with the image -->
    <!-- Replace 'REGISTRYURL' with the image regsitry URL -->
    <!-- Replace 'PROJECTURL' with the application project URL -->
    <!-- Replace 'OVERVIEWTEXT' with the description of the application -->
    <!-- App info -->
    <Name>wh-APPNAME</Name>
    <Overview>OVERVIEWTEXT</Overview>
    <Category>CACATEGORY</Category>
    <Beta>false</Beta>
    <!-- Image settings -->
    <Repository>REPONAME</Repository>
    <Registry>REGISTRYURL</Registry>
    <Project>PROJECTURL</Project>
    <!-- Docker settings -->
    <ExtraParams>--init --no-healthcheck --restart=always</ExtraParams>
    <Network>bridge</Network>
    <Privileged>false</Privileged>
    <PrivilegedReq>false</PrivilegedReq>
    <Shell>bash</Shell>
    <!-- Template settings -->
    <GitHub>https://github.com/whallin/unraid-templates</GitHub>
    <ReadMe>https://github.com/whallin/unraid-templates</ReadMe>
    <Support>https://github.com/whallin/unraid-templates/issues</Support>
    <Icon>https://raw.githubusercontent.com/whallin/unraid-templates/main/whallin/imgs/APPNAME.png</Icon>
    <TemplateURL>https://raw.githubusercontent.com/whallin/unraid-templates/main/whallin/APPNAME.xml</TemplateURL>
    <Date>2000-01-01</Date>
    <!-- Ports -->
    <!-- Volumes -->
    <!-- Variables -->
</Container>