<?xml version="1.0"?>
<Container version="2">
    <!-- App info -->
    <Name>wh-packetstream</Name>
    <Overview>PacketStream is a peer-to-peer proxy network that enables users to access data and content from any location.</Overview>
    <Category>Crypto: Other: Network:Other</Category>
    <Beta>false</Beta>
    <Requires>&#xD;PacketStream requires:&#xD;
    - A "packetstream.io" account.</Requires>
    <!-- Image settings -->
    <Repository>packetstream/psclient:latest</Repository>
    <Registry>https://hub.docker.com/r/packetstream/psclient</Registry>
    <Project>https://packetstream.io/</Project>
    <!-- Docker settings -->
    <ExtraParams>--init --no-healthcheck</ExtraParams>
    <Network>host</Network>
    <Privileged>false</Privileged>
    <PrivilegedReq>false</PrivilegedReq>
    <Shell>bash</Shell>
    <!-- Template settings -->
    <GitHub>https://github.com/whallin/unraid-templates</GitHub>
    <ReadMe>https://github.com/whallin/unraid-templates</ReadMe>
    <Support>https://github.com/whallin/unraid-templates/issues</Support>
    <Icon>https://raw.githubusercontent.com/whallin/unraid-templates/main/whallin/imgs/packetstream.png</Icon>
    <TemplateURL>https://raw.githubusercontent.com/whallin/unraid-templates/main/whallin/packetstream.xml</TemplateURL>
    <Date>2000-01-01</Date>
    <!-- Variables -->
    <Config Name="CID" Target="CID" Default="" Mode="" Description="Your PacketStream referral code to register your machine for your PacketStream account." Type="Variable" Display="always" Required="true" Mask="false"/>
</Container>