<?xml version="1.0"?>
<Container version="2">
    <!-- App info -->
    <Name>wh-mitmproxy</Name>
    <Overview>mitmproxy is a free and open source tool for debugging, testing, and hacking web traffic.</Overview>
    <Category>Security: Network:Web Network:Proxy Network:Other</Category>
    <Beta>false</Beta>
    <!-- Image settings -->
    <Repository>mitmproxy/mitmproxy:latest</Repository>
    <Registry>https://hub.docker.com/r/mitmproxy/mitmproxy</Registry>
    <Project>https://mitmproxy.org/</Project>
    <!-- Docker settings -->
    <WebUI>http://[IP]:[PORT:8081]/</WebUI>
    <ExtraParams>--init --no-healthcheck</ExtraParams>
    <PostArgs>mitmweb --web-host 0.0.0.0</PostArgs>
    <Network>host</Network>
    <Privileged>false</Privileged>
    <PrivilegedReq>false</PrivilegedReq>
    <Shell>bash</Shell>
    <!-- Template settings -->
    <GitHub>https://github.com/whallin/unraid-templates</GitHub>
    <ReadMe>https://github.com/whallin/unraid-templates</ReadMe>
    <Support>https://github.com/whallin/unraid-templates/issues</Support>
    <Icon>https://raw.githubusercontent.com/whallin/unraid-templates/main/whallin/imgs/mitmproxy.png</Icon>
    <TemplateURL>https://raw.githubusercontent.com/whallin/unraid-templates/main/whallin/mitmproxy.xml</TemplateURL>
    <Date>2000-01-01</Date>
</Container>